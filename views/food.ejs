<html lang="en">

<head>
    <title>Document</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>

<body style="background-color: bisque;">
    <div style="margin-left: 2%; margin-right: 2%;">
        <div class="row">
        <% food.details.forEach((item)=> { %>
            
                <div class="col-sm-3">
                    <div class="card" style="width: 18rem;">
                        <img class="card-img-top" src="<%= item.type %>.jpg" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title">
                                <%= item.name %>
                            </h5>
                            <p class="card-text">
                                <li> Amount: <%= item.quantity %>
                                </li>
                                <li> Contact Details: <%= item.contact %>
                                </li>
                                <li> Location: <%= item.location %>
                                </li>
                            </p>
                            <a href="#" class="btn btn-primary">Interested</a>
                        </div>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
</body>

</html>